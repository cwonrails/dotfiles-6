[alias]
  ms = config user.email \"maximbaz@microsoft.com\"
[branch]
  autosetuprebase = always
[core]
  autocrlf = input
  filemode = true
  pager = diff-so-fancy | less --tabs=4 -RFX
[merge]
  conflictstyle = diff3
  tool = vimdiff
[mergetool]
  keepBackup = false
  prompt = false
[push]
  default = current
[rebase]
  autostash = true
  autosquash = true
[user]
  useConfigOnly = true

